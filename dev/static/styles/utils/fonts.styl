/* neo-sans-pro-200 - cyrillic_latin */
@font-face {
    font-family: 'neo_sans_pro_light';
    font-style: normal;
    font-weight: 200;
    src: url('../fonts/neosanspro-light/NeoSansProLight.eot'); /* IE9 Compat Modes */
    src: local('Neo Sans Pro Light'), local('NeoSansPro-Light'),
        url('../fonts/neosanspro-light/NeoSansProLight.eot?#iefix') format('embedded-opentype'), /* IE6-IE8 */
        url('../fonts/neosanspro-light/NeoSansProLight.woff2') format('woff2'), /* Super Modern Browsers */
        url('../fonts/neosanspro-light/NeoSansProLight.woff') format('woff'), /* Modern Browsers */
        url('../fonts/neosanspro-light/NeoSansProLight.ttf') format('truetype'), /* Safari, Android, iOS */
        url('../fonts/neosanspro-light/NeoSansProLight.svg#NeoSansPro') format('svg'); /* Legacy iOS */
}
